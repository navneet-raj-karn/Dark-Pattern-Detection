{
  "manifest_version": 2,
  "name": "Safe Browse",
  "version": "1.0",
  "browser_action": {
    "default_title": "Highlight Dark Pattens",
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "file:///*"
  ],
  "web_accessible_resources": [
    "dataset.txt",
    "popup.html",
    "highlighter.js"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
